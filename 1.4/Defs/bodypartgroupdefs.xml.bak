<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RecipeDef Abstract="True" ParentName="BaseMechanoidRecipe" Name="BaseMechanoidRefurbishRecipe">
	<jobString>Refurbishing mech.</jobString>
	<fixedIngredientFilter>
		<specialFiltersToAllow>
			<li>AllowCorpsesMechEnemy</li>
		</specialFiltersToAllow>
	</fixedIngredientFilter>
	<ingredients>
		<li>
			<filter>
				<categories>
					<li>Corpses</li>
				</categories>
			</filter>
			<count>2</count>
		</li>
	</ingredients>
	<forceHiddenSpecialFilters>
		<li>AllowCorpsesMechEnemy</li>
	</forceHiddenSpecialFilters>
</RecipeDef>

<RecipeDef ParentName="BaseMechanoidRefurbishRecipe">
	<defName>MilitorRefurb</defName>
	<label>Refurbish militor</label>
	<description>Refurbish an enemy militor mechanoid. Two mechanoid bodies of the same model are combined into one useable mech.</description>
	<fixedIngredientFilter>
		<thingDefs>
			<li>Corpse_Mech_Militor</li>
		</thingDefs>
	</fixedIngredientFilter>
	<products>
		<Mech_Militor>1</Mech_Militor>
	</products>
	<descriptionHyperlinks>
		<ThingDef>Mech_Militor</ThingDef>
	</descriptionHyperlinks>
</RecipeDef>

<RecipeDef ParentName="BaseMechanoidRefurbishRecipe">
	<defName>LifterRefurb</defName>
	<label>Refurbish lifter</label>
	<description>Refurbish an enemy lifter mechanoid. Two mechanoid bodies of the same model are combined into one useable mech.</description>
		<fixedIngredientFilter>
		<thingDefs>
			<li>Corpse_Mech_Militor</li>
		</thingDefs>
	</fixedIngredientFilter>
	<products>
		<Mech_Lifter>1</Mech_Lifter>
	</products>
	<descriptionHyperlinks>
		<ThingDef>Mech_Lifter</ThingDef>
	</descriptionHyperlinks>
</RecipeDef>


	
</Defs>